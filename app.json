{
  "expo": {
    "name": "reactnative-authentication-sample",
    "slug": "reactnative-authentication-sample",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.shayanypn.reactnative-authentication-sample"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
